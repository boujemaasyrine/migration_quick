<!DOCTYPE html>
<html lang="{{ app.request.locale|split('_')[0] }}">
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        {% block title %}

            {% if version == "quick" %}
                Quick!
            {% else %}
                BK!
            {% endif %}

        {% endblock %}
    </title>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('plugins/jquery-ui/themes/base/jquery-ui.min.css') }}">
        <link rel="stylesheet" type="text/css"
              href="{{ asset('bundles/app/lib/quick-theme/jquery-ui-1.9.2.custom.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('plugins/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('plugins/Materialize/dist/css/materialize.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/main.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/lib/flags/flags.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('plugins/superfish/dist/css/superfish.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('plugins/superfish/dist/css/superfish-vertical.css') }}">
        <link rel="stylesheet" type="text/css"
              href="{{ asset('plugins/datatables/media/css/dataTables.bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/lib/progress-wizard.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/multi_select_modified.css') }}">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/app/images/favicon.png') }}"/>
</head>
<body>

<div id="loader" class="loader" style="display:none ;width: 100%; height: 100%;
    background: -webkit-linear-gradient(left top, rgba(242, 234, 217,1), rgba(255, 253, 255,1), rgba(228, 217, 195,1)); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(rgba(242, 234, 217,1), rgba(255, 253, 255,1), rgba(228, 217, 195,1)); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(rgba(242, 234, 217,1), rgba(255, 253, 255,1), rgba(228, 217, 195,1)); /* For Firefox 3.6 to 15 */
    background: linear-gradient(to bottom left,rgba(242, 234, 217,1), rgba(255, 253, 255,1), rgba(228, 217, 195,1)); /* Standard syntax */
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9999;">
    <div class="loader_background" style="position: absolute;height: 17em;left: 44%; overflow: hidden; width: 0;
    top: 35%;">
        <img src="{{ asset('bundles/app/images/background_loader.png') }}" style=" height:17em;">
    </div>

    <img class="loader_front" src="{{ asset('bundles/app/images/loader.png') }}" style="position: absolute;height: 17em;left: 44%;
    top: 35%;">
</div>

<div class="wrapper" style="min-height: 100%;
    position: relative;">


    {% block header %}

        <div class="row header">
            <div class="col s1" style="padding-left: 2%;">
                <a href="{{ path('home') }}">
                    <img class="logo" src="{{ asset('bundles/app/images/logo.png') }}">
                </a>
            </div>

            <div class="col s11 pull-right" style="padding-top: 20px;">
                {% block inner_header %}
                    <div class="col s9 pull-right" style="padding: 0;">
                        <div class="logout-zone pull-right" style="margin-left: 5px; margin-right: 10px;">
                            {% block logout %}
                            {% endblock %}
                        </div>
                        <a href="#aide-box-content"
                           class="strech_btn modal-trigger btn white black-text text-darken-2 waves-effect waves-light pull-right"
                           style="margin-left:5px;">
                            <i class="material-icons left" style="margin-right: 2px;">live_help</i><span
                                    class="hide-on-med-and-down">Aide</span>
                        </a>

                        <div class="lang_btn_zone pull-right" style="margin-left:5px;">
                            {% block lang_btn_zone %}
                            {% endblock %}
                        </div>
                        <div class="pull-right" style="margin-left:5px;">
                            {% block notification_zone %}
                                <a class="strech_btn modal-trigger btn red darken-3 white-text text-darken-2 waves-effect waves-red"
                                   href="#notifications-modal-box">
                                    <i class="material-icons left" style="margin-right: 2px;">add_alert</i> <span
                                            class="hide-on-med-and-down"> Notifications</span>
                                    (3)
                                </a>
                            {% endblock %}
                        </div>
                    </div>
                    <div class="col s3 pull-right hide-on-med-and-down">
                    <span class="text-danger" style=""><span class="glyphicon glyphicon-earphone"></span><strong>
                            HOTLINE: Support ECall</strong></span><br>
                    <span style="margin-right: 20px ;"><img src="{{ asset('bundles/app/images/blank.png') }}"
                                                            class="flag flag-be"> <b>0800 77877</b></span>
                        <span><img src="{{ asset('src/images/blank.png') }}" class="flag flag-lu"> <b>800 211
                                64</b></span>
                    </div>
                {% endblock %}
            </div>
        </div>

    {% endblock %}

    {% block vertical_nav %}
    {% endblock %}

    <div class="container main-content" style="    margin-right: 0;
    width: 100%;
    margin-left: 0;
    max-width: inherit;">
        {% block base_body %}
        {% endblock %}
    </div>

    {% block footer %}
    <div class="footer">
        <div style="" class="logo_footer">
            <a href="http://www.quick.fr/fr" target="_blank">
                <img class="logo" src="{{ asset('bundles/app/images/logo_footer.png') }}">
            </a>
        </div>
    </div>
    {% endblock %}
</div>

{% include ':parts:modals.html.twig' %}

{% block javascripts %}

    {#Third Party modules#}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    <script src="{{ asset('plugins/jquery/dist/jquery.min.js') }}"></script>
    <script src="{{ asset('plugins/jquery-ui/jquery-ui.js') }}"></script>
    <script src="{{ asset('plugins/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('plugins/moment/min/moment.min.js') }}"></script>
    <script src="{{ asset('plugins/moment/locale/fr.js') }}"></script>
    <script src="{{ asset('plugins/Materialize/dist/js/materialize.min.js') }}"></script>
    <script src="{{ asset('plugins/superfish/dist/js/superfish.js') }}"></script>
    <script src="{{ asset('plugins/datatables/media/js/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('plugins/datatables/media/js/dataTables.bootstrap.min.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/app/lib/jquery.uix.multiselect-master/js/jquery.uix.multiselect.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/app/lib/jquery.uix.multiselect-master/js/locales/jquery.uix.multiselect_fr.js') }}"></script>
    <script src="{{ asset('plugins/blockUI/jquery.blockUI.js') }}"></script>
    <script src="{{ asset('plugins/noty/js/noty/packaged/jquery.noty.packaged.min.js') }}"></script>
    <script src="{{ asset('plugins/noty/js/noty/themes/bootstrap.js') }}"></script>

    <script src="{{ asset('bundles/bazingajstranslation/js/translator.min.js') }}"></script>
    <script src="{{ url('bazinga_jstranslation_js') }}"></script>

    {#Common modules#}
    <script src="{{ asset('bundles/app/js/Module/common.js') }}"></script>
    <script src="{{ asset('bundles/app/js/Module/datepicker.js') }}"></script>
    <script src="{{ asset('bundles/app/js/Module/notificator.js') }}"></script>
    <script src="{{ asset('bundles/app/js/Module/loader_controller.js') }}"></script>
    <script src="{{ asset('bundles/app/js/Module/shortcut_controller.js') }}"></script>
    <script src="{{ asset('bundles/app/js/Module/navbar_managing.js') }}"></script>
    <script src="{{ asset('bundles/app/js/Module/datatables.js') }}"></script>


{% endblock %}
</body>
</html>
