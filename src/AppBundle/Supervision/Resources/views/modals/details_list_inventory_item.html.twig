<div class="detail-modal-body" style="padding: 5px;">
    <div class="row">
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'label.code' | trans({}, "supervision") }}: </span>
            {{ item.externalId }}
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'label.name' | trans({}, "supervision") }} FR: </span>
            {{ item.nameTranslation('fr') }}
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'label.name' | trans({}, "supervision") }} NL: </span>
            {{ item.nameTranslation('nl') }}
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'item.label.buying_cost' | trans({}, "supervision") }}: </span>
            {{ item.buyingCost }} &euro;
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'item.label.category' | trans({}, "supervision") }}: </span>
            {{ item.productCategory.name }}
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'item.label.status' | trans({}, "supervision") }}: </span>
            {{ ('status.' ~ item.status) | trans({}, "supervision") }}
        </div>
        {% if item.deactivationDate is not null %}
            <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
                <span class="label">{{ 'item.inventory.deactivateDate' | trans({}, "supervision") }}: </span>
                {{ item.deactivationDate | date("d/m/Y") }}
            </div>
        {% endif %}

    </div>
    <div class="row">
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'item.label.unit_expedition' | trans({}, "supervision") }}: </span>
            {{ item.labelUnitExped | trans }}
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'item.label.unit_inventory' | trans({}, "supervision") }}: </span>
            {{ item.labelUnitInventory | trans }}
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'item.label.unit_usage' | trans({}, "supervision") }}: </span>
            {{ item.labelUnitUsage | trans }}
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">

        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'item.label.inventory_qty' | trans({}, "supervision") }}: </span>
            {{ item.inventoryQty }}
        </div>
        <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
            <span class="label">{{ 'item.label.usage_qty' | trans({}, "supervision") }}: </span>
            {{ item.usageQty }}
        </div>
        {% if item.secondaryItem is not null %}
            <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
                <span class="label">{{ 'item.inventory.secondary_product' | trans({}, "supervision") }}: </span>
                {{ item.secondaryItem.name }}
            </div>
        {% endif %}
        {% if item.dateSynchro != null %}
            <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12 col-modal">
                <span class="label">{{ 'item.inventory.synchro_date' | trans({}, "supervision") }}: </span>
                {{ item.dateSynchro | date('d/m/Y') }}
            </div>
        {% endif %}

        {% if item.startDateCmd != null %}
            <div class="col col-lg-4 col-md-3 col-sm-6 col-xs-12 col-modal">
                <span class="label">{{ 'item.inventory.startDateCmdItem' | trans({}, "supervision") }}: </span>
                {{ item.startDateCmd | date('d/m/Y') }}
            </div>
        {% endif %}
        <div class="col col-lg-4 col-md-3 col-sm-6 col-xs-12 col-modal"><span class="label"> </span></div>
        <div class="col col-lg-4 col-md-3 col-sm-6 col-xs-12 col-modal"><span class="label"> </span></div>

        {% if item.endDateCmd != null %}
            <div class="col col-lg-4 col-md-3 col-sm-6 col-xs-12 col-modal">
                <span class="label">{{ 'item.inventory.endDateCmdItem' | trans({}, "supervision") }}: </span>
                {{ item.endDateCmd | date('d/m/Y') }}
            </div>
        {% endif %}
    </div>
    <div class="row">
        <div class="col-xs-2">
            <span class="label">{{ 'item.label.reusable' | trans({}, "supervision") }}</span>
        </div>
        <div class="col-xs-4">
            <div class="form-group">
                <input id="reusable" style="margin-top: 30px" type="checkbox" {% if item.reusable == 1 %} checked {% else %}  {% endif %}
                       readonly/>
                <label style="cursor: auto" for="#reusable"></label>
            </div>
        </div>
    </div>
    <br/>

    <div class="row">
        <div class="col col-xs-12">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li role="presentation" class="active"><a href="#restaurants" aria-controls="ticket_restaurant"
                                                          role="tab"
                                                          data-toggle="tab">
                        {{ 'keyword.restaurants'|trans({}, "supervision") }}
                    </a>
                </li>
                <li role="presentation"><a href="#suppliers" aria-controls="foreign_currency" role="tab"
                                           data-toggle="tab">
                        {{ 'restaurant.list.supplier_list'|trans({}, "supervision") }}
                    </a>
                </li>
            </ul>

            <!-- Tab panes -->
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="restaurants">
                    <table id="item_restaurants_table" class="table table-striped table-bordered table-hover"
                           cellspacing="0"
                           width="100%">
                        <thead>
                        <tr>
                            <th>{{ 'label.code' | trans({}, "supervision") }}</th>
                            <th>{{ 'label.name' | trans({}, "supervision") }}</th>
                            <th>{{ 'label.mail' | trans({}, "supervision") }}</th>
                            <th>{{ 'label.manager' | trans({}, "supervision") }}</th>
                            <th>{{ 'label.address' | trans({}, "supervision") }}</th>
                            <th>{{ 'label.phone' | trans({}, "supervision") }}</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for restaurant in item.restaurants %}
                            <tr id="{{ restaurant.id }}">
                                <td>{{ restaurant.code }}</td>
                                <td>{{ restaurant.name }}</td>
                                <td>{{ restaurant.email }}</td>
                                <td>{{ restaurant.manager }}</td>
                                <td>{{ restaurant.address }}</td>
                                <td>{{ restaurant.phone }}</td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
                <div role="tabpanel" class="tab-pane" id="suppliers">
                    <table id="supplier_resto_list_table" class="table table-striped table-bordered table-hover"
                           cellspacing="0"
                           width="100%">
                        <thead>
                        <tr>
                            <th>{{ 'label.code' | trans({}, "supervision") }}</th>
                            <th>{{ 'provider.list.provider' | trans({}, "supervision") }}</th>
                            <th>{{ 'provider.list.address' | trans({}, "supervision") }}</th>
                            <th>{{ 'provider.list.phone' | trans({}, "supervision") }}</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for supplier in item.suppliers %}
                            <tr id="{{ supplier.id }}">
                                <td>{{ supplier.code }}</td>
                                <td>{{ supplier.name }}</td>
                                <td>{{ supplier.address }}</td>
                                <td>{{ supplier.phone }}</td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

