<div class="detail-modal-body" style="padding: 5px;">
    <div class="row">
        <div class="col-modal col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <span class="label">{{ 'label.code' | trans }} : </span>
            {{ restaurant.code }}
        </div>
        <div class="col-modal col-lg-6 col-md-6 col-sm-6 col-xs-12" style="text-align: right;">
            <span class="label">{{ 'label.name' | trans }} : </span>
            {{ restaurant.name }}
        </div>
        <div class="col-modal col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <span class="label">{{ 'label.mail' | trans }} : </span>
            {{ restaurant.email }}
        </div>
        <div class="col-modal col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <span class="label">{{ 'label.phone' | trans }} : </span>
            {{ restaurant.phone }}
        </div>
        <div class="col-modal col-lg-6 col-md-6 col-sm-6 col-xs-12" style="text-align: right;">
            <span class="label">{{ 'label.manager' | trans }} : </span>
            {{ restaurant.manager }}
        </div>
        <div class="col-modal col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <span class="label">{{ 'label.type' | trans }} : </span>
            {{ restaurant.type }}
        </div>
        <div class="col-modal col-lg-6 col-md-6 col-sm-6 col-xs-12" style="text-align: right;">
            <span class="label">{{ 'label.managerPhone' | trans }} : </span>
            {{ restaurant.managerPhone }}
        </div>
        <div class="col-modal col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <span class="label">{{ 'label.opening_date' | trans([],'supervision' ) }} : </span>
            {% if openingDate.timestamp is defined %}
                {{ openingDate|date('d/m/Y') }}
            {% else %}
                -
            {% endif %}
        </div>
        <div class="col-modal col-lg-6 col-md-6 col-sm-6 col-xs-12" style="text-align: right;">
            <span class="label">{{ 'label.managerEmail' | trans }} : </span>
            {{ restaurant.managerEmail }}
        </div>

    </div>
    <div class="row">
        <div class="col-xs-2">
            <span class="label">{{ 'label.reusable' | trans({}, "supervision") }}</span>
        </div>
        <div class="col-xs-4">
            <div class="form-group">
                <input id="reusable" type="checkbox" {% if restaurant.reusable == 1 %} checked {% endif %}
                       readonly/>
                <label style="cursor: auto" for="#reusable"></label>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col col-xs-12">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li role="presentation" class="active"><a href="#suppliers" aria-controls="foreign_currency" role="tab"
                                                          data-toggle="tab">
                        {{ 'restaurant.list.supplier_list'|trans({}, "supervision") }}
                    </a>
                </li>
                <li role="presentation"><a href="#restaurants" aria-controls="ticket_restaurant" role="tab"
                                           data-toggle="tab">
                        {{ 'keyword.wynd_informations'|trans({}, "supervision") }}
                    </a>
                </li>
            </ul>

            <!-- Tab panes -->
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="suppliers">
                    <table id="supplier_resto_list_table" class="table table-striped table-bordered table-hover"
                           cellspacing="0"
                           width="100%">
                        <thead>
                        <tr>
                            <th>{{ 'label.code' | trans }}</th>
                            <th>{{ 'provider.list.provider' | trans }}</th>
                            <th>{{ 'provider.list.designation' | trans }}</th>
                            <th>{{ 'provider.list.address' | trans }}</th>
                            <th>{{ 'provider.list.phone' | trans }}</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for supplier in restaurant.suppliers %}
                            <tr id="{{ supplier.id }}">
                                <td>{{ supplier.code }}</td>
                                <td>{{ supplier.name }}</td>
                                <td>{{ supplier.designation }}</td>
                                <td>{{ supplier.address }}</td>
                                <td>{{ supplier.phone }}</td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>

                <div role="tabpanel" class="tab-pane" id="restaurants">
                    <div class="row">
                        <div class="col-xs-12" style="margin-top: 20px">
                            <span class="label">- {{ 'label.orders_url' | trans({}, "supervision") }}: </span>
                            {{ ordersUrl }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <span class="label">- {{ 'label.users_url' | trans({}, "supervision") }}: </span>
                            {{ usersUrl }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <span class="label">- {{ 'label.withdrawal_url' | trans({}, "supervision") }}: </span>
                            {{ withdrawalUrl }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <span class="label">- {{ 'label.optikitchen_path' | trans({}, "supervision") }}: </span>
                            {{ optikitchenPath }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <span class="label">- {{ 'label.wynd_user' | trans({}, "supervision") }} : </span>
                            {{ wyndUser }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <span class="label">- {{ 'label.secret_key' | trans({}, "supervision") }} : </span>
                            {{ secretKey }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-2">
                            <span class="label">- {{ 'label.wynd_active' | trans({}, "supervision") }}</span>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <input id="wynd-active" type="checkbox" {% if wyndActive == 1 %} checked {% endif %}
                                       readonly/>
                                <label style="cursor: auto" for="#wynd-active"></label>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>


